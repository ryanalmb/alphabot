{
  "name": "alpha-pack",
  "version": "1.0.0",
  "description": "Enterprise-grade Solana-first social trading ecosystem",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "jest --testPathPattern=e2e",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "cdk": "cdk",
    "deploy": "cdk deploy --all",
    "destroy": "cdk destroy --all",
    "bootstrap": "cdk bootstrap",
    "synth": "cdk synth",
    "diff": "cdk diff",
    "start:api": "node dist/api/server.js",
    "start:bot": "node dist/telegram/bot.js",
    "dev:api": "nodemon --exec ts-node src/api/server.ts",
    "dev:bot": "nodemon --exec ts-node src/telegram/bot.ts",
    "build:solana": "anchor build",
    "test:solana": "anchor test",
    "deploy:solana": "anchor deploy"
  },
  "keywords": [
    "solana",
    "defi",
    "social-trading",
    "aws",
    "enterprise",
    "telegram",
    "ml",
    "ai"
  ],
  "author": "Alpha Pack Team",
  "license": "MIT",
  "dependencies": {
    "aws-cdk-lib": "^2.100.0",
    "constructs": "^10.3.0",
    "@solana/web3.js": "^1.87.6",
    "@solana/spl-token": "^0.3.8",
    "@project-serum/anchor": "^0.28.0",
    "@coral-xyz/anchor": "^0.28.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "telegraf": "^4.15.6",
    "redis": "^4.6.10",
    "pg": "^8.11.3",
    "mongodb": "^6.2.0",
    "@aws-sdk/client-dynamodb": "^3.450.0",
    "@aws-sdk/lib-dynamodb": "^3.450.0",
    "@aws-sdk/client-bedrock": "^3.450.0",
    "@aws-sdk/client-bedrock-runtime": "^3.450.0",
    "@aws-sdk/client-sagemaker": "^3.450.0",
    "@aws-sdk/client-sagemaker-runtime": "^3.450.0",
    "@aws-sdk/client-comprehend": "^3.450.0",
    "@aws-sdk/client-rekognition": "^3.450.0",
    "@aws-sdk/client-secretsmanager": "^3.450.0",
    "@aws-sdk/client-eventbridge": "^3.450.0",
    "@aws-sdk/client-s3": "^3.450.0",
    "@aws-sdk/client-sns": "^3.450.0",
    "@aws-sdk/client-sqs": "^3.450.0",
    "axios": "^1.6.0",
    "ws": "^8.14.2",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "joi": "^17.11.0",
    "winston": "^3.11.0",
    "express-rate-limit": "^7.1.5",
    "compression": "^1.7.4",
    "express-validator": "^7.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.8.7",
    "@types/express": "^4.17.20",
    "@types/cors": "^2.8.15",
    "@types/morgan": "^1.9.7",
    "@types/pg": "^8.10.7",
    "@types/ws": "^8.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.6",
    "@types/aws-lambda": "^8.10.130",
    "@types/compression": "^1.7.5",
    "typescript": "^5.2.2",
    "ts-node": "^10.9.1",
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "aws-cdk": "^2.100.0",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.16"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
